process {

     //tanaka,cluster
     queue='pr-el7,short-sl7,long-sl7'
     cpus='1'
     scratch = false
     errorStrategy = 'ignore'
     executor = 'sge'

     withLabel: thermoconvert {
        container = 'quay.io/biocontainers/thermorawfileparser:1.2.3--1'
     }

     withLabel: openms {
        container = 'openms/executables:latest'
        clusterOptions = '-l h_rt=120:00:00,virtual_free=9G'
     }

     withLabel: ffm {
        container = 'openms/executables:latest'
        clusterOptions = '-l h_rt=120:00:00,virtual_free=9G'
     }

     withLabel: mascot {
        container = 'openms/executables:latest'
        clusterOptions = '-l h_rt=120:00:00,virtual_free=14G'
     }

     withLabel: comet {
        container = 'openms/executables:latest'
     }

     withLabel: diaumpire {
        clusterOptions = '-l h_rt=120:00:00,virtual_free=9G -pe smp 4 -m abe'
     }

     withLabel: clitools {
        clusterOptions = '-l virtual_free=9G'
        container = 'proteomicsunitcrg/clitools:latest'
     }

}

profiles {
	test { includeConfig 'conf/test.config' }
}

workDir = "$baseDir/work"
includeConfig "conf/params.config"
includeConfig "conf/openms.config"
includeConfig "conf/dia_umpire.config"
singularity.cacheDir = "$baseDir/singularity"
singularity.enabled = true
